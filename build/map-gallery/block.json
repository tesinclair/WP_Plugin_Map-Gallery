{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "map-gallery/map-gallery",
  "version": "0.1.0",
  "title": "Map Gallery",
  "category": "widgets",
  "description": "Map Gallery Widget that displays images on map using geolocation data.",
  "example": {},
  "attributes": {
    "hoverGrowScale": {
      "type": "number",
      "default": 1.1
    },
    "mapHeight": {
      "type": "number",
      "default": 500
    },
    "mapWidth": {
      "type": "number",
      "default": 0
    },
    "imgIconInitialHeight": {
      "type": "number",
      "default": 20
    },
    "imgIconInitialWidth": {
      "type": "number",
      "default": 10
    }
  },
  "supports": {
    "color": {
      "background": false,
      "text": true
    },
    "html": false,
    "typography": {
      "fontSize": true
    }
  },
  "textdomain": "map-gallery",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}